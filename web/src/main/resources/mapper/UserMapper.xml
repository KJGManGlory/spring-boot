<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lizza.web.dao.UserDao">

    <insert id="insert">

    </insert>
    <select id="select" resultType="com.lizza.web.entity.User">
        select * from user where id = #{id}
    </select>
    <select id="list" resultType="com.lizza.web.entity.User">
        select * from user where id between #{param1} and #{param2}
    </select>
</mapper>